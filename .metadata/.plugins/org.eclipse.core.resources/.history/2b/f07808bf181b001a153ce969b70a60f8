		   if (choice == JOptionPane.NO_OPTION)
		   {
			   JOptionPane.showMessageDialog(null, "OK_sir");
			   
		   }
		   else
		   {
			   
			   JOptionPane.showMessageDialog(null, "NOT_OK");
		   }
		  }
	   /**
	    * @wbp.parser.entryPoint
	    */
	   static void test_loop()
	   {
		   JFrame frame = new JFrame();
